{
  "tagging_enabled": true,
  "default_grid_dimensions": {
    "nx": 100,
    "ny": 200,
    "nz": 300
  },
  "default_resolution": {
    "dx": 0.01,
    "dy": 0.01,
    "dz": 0.01
  },
  "bounding_volume": 512.0,
  "metadata_output_path": "output/enriched_metadata.json",
  "pipeline_index_path": "configs/metadata/pipeline_index.json",
  "logging": {
    "level": "INFO",
    "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
  }
}



